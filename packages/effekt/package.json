{
  "name": "effekt",
  "version": "0.10.0",
  "author": "Ivo Dolenc",
  "description": "Universal interaction platform for the modern web.",
  "license": "MIT",
  "repository": "effekt-labs/effekt",
  "homepage": "https://github.com/effekt-labs/effekt",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.mts",
      "import": "./dist/utils/index.mjs"
    },
    "./easing": {
      "types": "./dist/easing/index.d.mts",
      "import": "./dist/easing/index.mjs"
    },
    "./sequence": {
      "types": "./dist/sequence/index.d.mts",
      "import": "./dist/sequence/index.mjs"
    },
    "./interaction": {
      "types": "./dist/interaction/index.d.mts",
      "import": "./dist/interaction/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "effekt",
    "effect",
    "animate",
    "animations",
    "web-animations",
    "interaction",
    "javascript",
    "typescript",
    "universal",
    "animation",
    "platform",
    "frame",
    "tween"
  ],
  "scripts": {
    "build": "hyperbundler",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "publish:effekt": "npm run build && npm publish"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@types/node": ">=20.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.14.2"
  }
}
